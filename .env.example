# Prime VPN Environment Variables
# Copy this file to .env and replace placeholders with actual values

# Backend API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:8000/api/v1

# WireGuard Server Configuration (REPLACE WITH ACTUAL VALUES)
EXPO_PUBLIC_SERVER_ENDPOINT=<SERVER_IP>:51820
EXPO_PUBLIC_SERVER_PUBLIC_KEY=<BASE64_SERVER_PUBLIC_KEY>
EXPO_PUBLIC_ROUTED_IP=<SERVER_ROUTED_IP>

# Encryption Keys (REPLACE WITH SECURE VALUES)
EXPO_PUBLIC_ENCRYPTION_KEY=<SECURE_ENCRYPTION_KEY>

# Payment Configuration (REPLACE WITH ACTUAL KEYS)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=<STRIPE_PUBLISHABLE_KEY>
EXPO_PUBLIC_PAYPAL_CLIENT_ID=<PAYPAL_CLIENT_ID>

# Ad Configuration (REPLACE WITH ACTUAL VALUES)
EXPO_PUBLIC_AD_PROVIDER=<AD_PROVIDER>
EXPO_PUBLIC_AD_UNIT_ID=<AD_UNIT_ID>
EXPO_PUBLIC_AD_INTERVAL=<AD_INTERVAL_SECONDS>

# VPN Protocol Configuration (REPLACE WITH ACTUAL VALUES)
EXPO_PUBLIC_VPN_PROTOCOL=<WireGuard>
EXPO_PUBLIC_VPN_ENCRYPTION=<ChaCha20Poly1305>

